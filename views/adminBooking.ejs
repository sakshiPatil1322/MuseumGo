        <!-- Booking Start -->
        <div class="container-xxl py-5">
            <div class="container">
                <div class="text-center wow fadeInUp" data-wow-delay="0.1s">
                    <h6 class="section-title text-center text-primary text-uppercase">TicketWise</h6>
                    <h1 class="mb-5">Book Your <span class="text-primary text-uppercase"> Museum Visit</span></h1>
                </div>
                <div class="row g-5">
                    <div class="col-lg-6">
                        <div class="row g-3">
                            <div class="col-6 text-end">
                                <img class="img-fluid rounded w-75 wow zoomIn" data-wow-delay="0.1s" src="img/booking2.jpg" style="margin-top: 25%;">
                            </div>
                            <div class="col-6 text-start">
                                <img class="img-fluid rounded w-100 wow zoomIn" data-wow-delay="0.3s" src="img/booking.jpg">
                            </div>
                            <div class="col-6 text-end">
                                <img class="img-fluid rounded w-50 wow zoomIn" data-wow-delay="0.5s" src="img/booking3.webp">
                            </div>
                            <div class="col-6 text-start">
                                <img class="img-fluid rounded w-75 wow zoomIn" data-wow-delay="0.7s" src="img/booking7.webp">
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="wow fadeInUp" data-wow-delay="0.2s">
                            <form method="post" action="/event" enctype="multipart/form-data">
                                <div class="row g-3">
                                    <div class="col-md-6">
                                        <div class="form-floating">
                                            <input type="text" name="evName" class="form-control" id="evName" placeholder="Event Name">
                                            <label for="evName">Event Name</label>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-floating">
                                            <input type="number" name="ppp" class="form-control" id="price" placeholder="Price">
                                            <label for="price">Price</label>
                                        </div>
                                    </div>
                                <div class="col-md-6">
                                    <div class="form-floating">
                                        <input type="text" name="aSeat" class="form-control" id="price" placeholder="Price">
                                        <label for="price">Available Seats</label>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-floating date" id="date3" data-target-input="nearest">
                                        <input type="date" class="form-control datetimepicker-input" id="checkin" name="evDate" placeholder="Check In" data-target="#date3" data-toggle="datetimepicker" required>
                                        <label for="checkin">Date of Event</label>
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="form-floating">
                                        <input type="text" name="smallInfo" class="form-control" id="smallInfo" placeholder="Little Information">
                                        <label for="smallInfo">Little Information</label>
                                    </div>
                                </div>
                                    <div class="col-12">
                                        <div class="form-floating">
                                            <textarea class="form-control" name="description" placeholder="Description" id="description" style="height: 100px"></textarea>
                                            <label for="description">Description</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label>Reference Image</label>
                                        <div class="image-drop-area" id="imageDropArea">
                                            <p>Drag & Drop Images Here or Click to Upload</p>
                                            <input type="file" id="imageUpload" name="image">
                                        </div>
                                    </div>    
                                    <div class="col-12">
                                        <button class="btn btn-primary w-100 py-3" type="submit">Add Event</button>
                                    </div>
                                </div>
                            </form>
                            
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Booking End -->


